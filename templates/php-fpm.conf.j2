; Global Options ;

[global]
;pid = run/php-fpm.pid
error_log = syslog
syslog.facility = daemon
syslog.ident = php-fpm-{{ php_version }}
log_level = error
;emergency_restart_threshold = 0
;emergency_restart_interval = 0
;process_control_timeout = 0
; process.max = 128
; process.priority = -19
daemonize = no
;rlimit_files = 1024
;rlimit_core = 0
;events.mechanism = epoll
;systemd_interval = 10

; Pool Definitions ;

include=/usr/local/php/{{ php_version }}/etc/php-fpm.d/*.conf
